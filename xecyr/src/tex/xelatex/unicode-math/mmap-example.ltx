\documentclass[12pt]{article}
\usepackage{fontspec}
\setromanfont{Cambria Math}
\setlength\parindent{0pt}
\setlength\parskip{1ex}
\newcommand\test[1]{%
  \addfontfeature{#1}
   abc def ghi jkl mno pqr stu vw xyz\\
   ABC DEF GHI JKL MNO PQR STU VW XYZ\\
   1234567890\par}
\begin{document}
\test{Mapping = math-italic}
\test{Mapping = math-bold}
\test{Mapping = math-italic-bold}
\test{Mapping = math-script}
\test{Mapping = math-script-bold}
\test{Mapping = math-blackboard}
\test{Mapping = math-fraktur}
\test{Mapping = math-fraktur-bold}
\test{Mapping = math-sans}
\test{Mapping = math-sans-bold}
\test{Mapping = math-sans-italic}
\test{Mapping = math-sans-italic-bold}
\test{Mapping = math-typewriter}
\end{document}
